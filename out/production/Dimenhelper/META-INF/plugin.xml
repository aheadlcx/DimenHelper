<idea-plugin version="2">
  <id>me.aheadlcx.dimenhelper</id>
  <name>DimenHelper</name>
  <version>1.3</version>
  <vendor email="aheadlcxzhang@gmail.com" url="https://github.com/aheadlcx/DimenHelper">aheadlcx</vendor>

  <description><![CDATA[
      create others value dimen file by your current dimen file.<br>
    ]]></description>
  <!--<em>Android TV dimen</em>-->

  <change-notes><![CDATA[
      refresh the file when file have benn created<br>
    ]]>
  </change-notes>
  <!--<em>most HTML tags may be used</em>-->

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="1.3"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  -->
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action class="me.aheadlcx.dimenhelper.MainAction" id="DimenHelper"
            text="DimenHelper"
    >
      <add-to-group group-id="ToolsMenu" anchor="first"></add-to-group>
      <keyboard-shortcut first-keystroke="$default" keymap="alt s"></keyboard-shortcut>
    </action>
  </actions>

</idea-plugin>